{
  "name": "leetime",
  "version": "1.0.0",
  "description": "A simple timer for leetcode",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/problems/*"
      ],
      "js": [
        "src/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
